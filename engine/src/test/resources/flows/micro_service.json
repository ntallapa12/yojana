{
  "id": "1234123443219",
  "name": "Send Email Service",
  "description": "Example user micro service",
  "variables": [
    {
      "name": "emailSubject",
      "type": "String",
      "isCollection": false,
      "scope": "Input"
    },
    {
      "name": "emailBody",
      "type": "String",
      "isCollection": false,
      "scope": "Input"
    },
    {
      "name": "emailTo",
      "type": "String",
      "isCollection": true,
      "scope": "Input"
    },
    {
      "name": "index",
      "type": "Integer",
      "isCollection": false,
      "scope": "Private"
    },
    {
      "name": "success",
      "type": "Boolean",
      "isCollection": false,
      "scope": "Output"
    }
  ],
  "flowObjects": [
    {
      "id": "5001",
      "name": "start process",
      "type": "StartEvent"
    },
    {
      "id": "5003",
      "name": "Initialize",
      "type": "InlineScript",
      "scriptRefId": "1429372764301"
    },
    {
      "id": "5005",
      "name": "More recipients?",
      "type": "DecisionGateway",
      "conditions": [
        {
          "when": "data.index < data.emailTo.length",
          "connectTo": "5006"
        },
        {
          "connectTo": "5011"
        }
      ]
    },
    {
      "id": "5007",
      "name": "Email recipient",
      "type": "SubFlow",
      "subFlowType": "MicroService",
      "address": "io.flowly.engine.email.send",
      "dataMappings": [
        {
          "from": "data.emailTo[data.index]",
          "to": "emailTo",
          "scope": "Input"
        },
        {
          "from": "data.emailSubject",
          "to": "emailSubject",
          "scope": "Input"
        },
        {
          "from": "data.emailBody",
          "to": "emailBody",
          "scope": "Input"
        }
      ]
    },
    {
      "id": "5009",
      "name": "Increment Index",
      "type": "InlineScript",
      "scriptRefId": "1429372764271"
    },
    {
      "id": "5012",
      "name": "End",
      "type": "EndEvent"
    }
  ],
  "connectingObjects": [
    {
      "id": "5002",
      "name": "sequence 1",
      "fromId": "5001",
      "toId": "5003"
    },
